; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Roll To Kill"
run/main_scene="res://scenes/screens/main_menu.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/main_scene="res://scenes/main.tscn"

[autoload]

Utils="*res://scripts/utils.gd"
EffectLibrary="*res://scripts/effect_library.gd"
StatusLibrary="*res://scripts/status_library.gd"
GameAnalyticsManager="*res://scripts/game_analytics_manager.gd"
GoogleAnalytics="*res://addons/google_analytics/analytics_singleton.gd"
Analytics="*res://addons/google_analytics/analytics_singleton.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/google_analytics/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
